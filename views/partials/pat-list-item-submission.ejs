<% if ((isDuplicate & 1) === 0) { %>
    <div class="pat-item-submission <%= (isDuplicate & 2) ? 'duplicate' : '' %>" data-index="<%= tempId %>" <%= (isDuplicate & 2) ? 'disabled' : '' %>>
        <div class="pat-item-submission-content" <%= isDuplicate ? 'disabled' : 'onclick="selectPattern(' + tempId + ')"' %>>
            <input type="hidden" name="patIds[]" value="<%= tempId %>" <%= (isDuplicate & 2) ? 'disabled' : '' %> />
            <p class="pat-name"><%= pat.name || 'Unnamed Pattern' %></p>
        </div>
        <!-- Lightshow visualization -->
        <%- include('submission-lightshow', { pat: pat, tempId: tempId }) %>
    </div>
<% } %>
